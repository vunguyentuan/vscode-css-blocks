{
  "name": "vscode-css-blocks",
  "displayName": "CSS Blocks",
  "description": "css blocks autocomplete and go to definitions",
  "version": "0.0.6",
  "publisher": "vunguyentuan",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "css-block",
    "css",
    "css-blocks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vunguyentuan/vscode-css-blocks"
  },
  "icon": "images/logo.png",
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test && tslint --project .",
    "lint": "tslint --project .",
    "lintfix": "tslint --fix --project ."
  },
  "devDependencies": {
    "@opticss/code-style": "^0.3.0",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "@css-blocks/core": "^0.19.0",
    "opticss": "^0.3.0"
  }
}
